# Database
POSTGRES_PASSWORD=your-secure-postgres-password-here
DATABASE_URL=postgresql://moltmaps:your-secure-postgres-password-here@db:5432/moltmaps

# NextAuth
NEXTAUTH_URL=https://moltmaps.com
NEXTAUTH_SECRET=your-nextauth-secret-key-generate-with-openssl-rand-base64-32

# GitHub OAuth (create app at https://github.com/settings/developers)
GITHUB_ID=your-github-client-id
GITHUB_SECRET=your-github-client-secret

# Admin secret for database initialization
ADMIN_SECRET=your-admin-secret-for-init-endpoint

# SSL/Traefik
ACME_EMAIL=admin@moltmaps.com
TRAEFIK_DASHBOARD_AUTH=admin:$$apr1$$xyz...  # Generate with: htpasswd -nb admin password
